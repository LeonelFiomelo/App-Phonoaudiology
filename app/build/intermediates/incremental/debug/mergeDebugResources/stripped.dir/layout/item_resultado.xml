<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/cl_resultadoItem"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="12dp"
    android:background="@drawable/ic_item_resultado_background"
    android:padding="10dp">

    <TextView
        android:id="@+id/tv_fecha_item"
        style="@android:style/Widget.TextView.PopupMenu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="@string/tv_fecha"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_fechaOutput_item"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="@+id/tv_fecha_item"
        app:layout_constraintTop_toBottomOf="@+id/tv_fecha_item" />

    <TextView
        android:id="@+id/tv_categoria_item"
        style="@android:style/Widget.TextView.PopupMenu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="@string/tv_categoria"
        app:layout_constraintStart_toStartOf="@+id/tv_fechaOutput_item"
        app:layout_constraintTop_toBottomOf="@+id/tv_fechaOutput_item" />

    <TextView
        android:id="@+id/tv_categoriaOutput_item"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="@+id/tv_categoria_item"
        app:layout_constraintTop_toBottomOf="@+id/tv_categoria_item" />

    <TextView
        android:id="@+id/tv_subcategoria_item"
        style="@android:style/Widget.TextView.PopupMenu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="@string/tv_subcategoria"
        app:layout_constraintStart_toStartOf="@+id/tv_categoriaOutput_item"
        app:layout_constraintTop_toBottomOf="@+id/tv_categoriaOutput_item" />

    <TextView
        android:id="@+id/tv_subcategoriaOutput_item"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="@+id/tv_subcategoria_item"
        app:layout_constraintTop_toBottomOf="@id/tv_subcategoria_item" />

    <TextView
        android:id="@+id/tv_ejercicio_item"
        style="@android:style/Widget.TextView.PopupMenu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="@string/tv_ejercicio"
        app:layout_constraintStart_toStartOf="@+id/tv_subcategoriaOutput_item"
        app:layout_constraintTop_toBottomOf="@+id/tv_subcategoriaOutput_item" />

    <TextView
        android:id="@+id/tv_ejercicioOutput_item"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="@+id/tv_ejercicio_item"
        app:layout_constraintTop_toBottomOf="@+id/tv_ejercicio_item" />

    <TextView
        android:id="@+id/tv_ruido_item"
        style="@android:style/Widget.TextView.PopupMenu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="@string/tv_ruido"
        app:layout_constraintStart_toStartOf="@+id/tv_ejercicioOutput_item"
        app:layout_constraintTop_toBottomOf="@+id/tv_ejercicioOutput_item" />

    <TextView
        android:id="@+id/tv_ruidoOutput_item"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="@+id/tv_ruido_item"
        app:layout_constraintTop_toBottomOf="@+id/tv_ruido_item" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="157dp"
        android:layout_height="146dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/tv_puntuacion_item"
            style="@android:style/Widget.TextView.PopupMenu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/tv_puntuacion"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/tv_puntuacionOutput_item"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:background="@drawable/ic_puntuacion_background"
            android:gravity="center"
            android:text="TextView"
            android:textColor="#FFFFFF"
            android:textSize="20sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_puntuacion_item" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>